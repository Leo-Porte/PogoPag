<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PogoPagApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PogoPagApp"
    xmlns:views="clr-namespace:PogoPagApp.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="PogoPagApp">

    <ShellContent Route="LoginPage" ContentTemplate="{DataTemplate views:LoginPage}" />
    <ShellContent Route="CadastroPage" ContentTemplate="{DataTemplate views:CadastroPage}" />

</Shell>
